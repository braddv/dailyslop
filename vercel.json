{
        "routes": [
          {
            "src": "/",
            "dest": "/dailyslop.html"
          }
        ],
  "rewrites": [
    {
      "source": "/:path*",
      "destination": "/dailyslop.html"
    }
  ],
  "headers": [
    {
      "source": "/dailyslop.mp4",
      "headers": [
        { "key": "Content-Type", "value": "video/mp4" }
      ]
    }
  ]
}