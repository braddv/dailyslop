{  
  "functions": {
    "api/**/*.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/slop",
      "destination": "/slop.html"
    },
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/slop1",
      "destination": "/slop1.html"
    },
    {
      "source": "/slop2",
      "destination": "/slop2.html"
    },
    {
      "source": "/slop3",
      "destination": "/slop3.html"
    }
  ],
  "headers": [
    {
      "source": "/dailyslop.mp4",
      "headers": [
        { "key": "Content-Type", "value": "video/mp4" }
      ]
    }
  ]
}
