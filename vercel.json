{
  "rewrites": [
    {
        "source": "public/:asset*",
        "destination": "public/:asset*"
    },
    {
      "source": "/:path*",
      "destination": "public/index.html"
    },
    {
        "source": "/slop1",
        "destination": "public/slop1.html"
    }
  ],
  "headers": [
    {
      "source": "/dailyslop.mp4",
      "headers": [
        { "key": "Content-Type", "value": "video/mp4" }
      ]
    }
  ],
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ]
}